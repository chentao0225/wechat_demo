.indexContainer{
  display: flex;
  flex-direction:column;
  align-items: center;
  height: 100%;
  background-color: #8ed145;
}
.userAvatar{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin: 100rpx 0;
}
.btn{
  width:200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  font-size: 20rpx;
  text-align: center;
  line-height: 200rpx;
  margin: 100rpx 0;
  background-color: skyblue;
}
.userName{
  margin:100rpx 0;
  font-size: 32rpx;
  font-weight: bold;
}
.goStudy{
  width:280rpx;
  height:80rpx;
  line-height: 80rpx;
  border:1px solid #eee;
  border-radius: 8rpx;
  font-size: 24rpx;
  text-align: center;
}