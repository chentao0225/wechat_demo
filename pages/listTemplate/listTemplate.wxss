/* pages/listTemplate/listTemplate.wxss */
.tmpContainer{
  display: flex;
  flex-direction: column;
  border: 1rpx solid #eee;
  padding-bottom: 10rpx;
  
}
.avatar_date{
  padding: 10rpx;
}
.avatar_date image{
  width:60rpx;
  height:60rpx;
  vertical-align: middle;
}
.avatar_date text{
  font-size: 26rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}

.company{
  font-weight: bold;
  font-size: 36rpx;
  margin-left: 10rpx;
}
.tmpImg{
  width: 100%;
  height: 460rpx;
}
.content{
  font-size: 26rpx;
  text-indent: 26rpx;
  line-height: 46rpx;
}
.star_view_container image{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.star_view_container text{
  font-size: 24rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}

